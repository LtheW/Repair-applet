<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.StudentAccountMapper">

    <resultMap id="BaseResultMap" type="com.example.demo.domain.StudentAccount">
            <id property="student_number" column="student_number" jdbcType="INTEGER"/>
            <result property="name" column="name" jdbcType="CHAR"/>
            <result property="passport" column="passport" jdbcType="CHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        student_number,name,passport
    </sql>
</mapper>
